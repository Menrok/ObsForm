@model ObsForm.Models.Dane

<h2>Wynik formularza</h2>

<div>
    <p><strong>Imię:</strong> @Model.Imie</p>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Temat:</strong> @Model.Temat</p>
